{
  "name": "super-app",
  "version": "0.1.0",
  "scripts": {
    "start": "NODE_ENV=development npx webpack serve",
    "build": "npx webpack",
    "test": "jest --passWithNoTests",
    "provision": "aws cloudformation deploy --region us-east-1 --template-file ./aws.yaml --stack-name super-app-stack --parameter-overrides BucketName=the-super-app AccountId=749936333804",
    "upload": "aws s3 sync dist/ s3://the-super-app --delete",
    "deploy": "npm run build && npm run provision && npm run upload"
  },
  "dependencies": {
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@tanstack/react-query": "^5.81.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-refresh": "^0.17.0",
    "react-router-dom": "^7.6.2",
    "react-spinners": "^0.17.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@tanstack/react-query-devtools": "^5.81.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "axios": "^1.10.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^30.0.3",
    "path": "^0.12.7",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
