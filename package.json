{
  "name": "super-app",
  "version": "0.1.0",
  "scripts": {
    "start": "NODE_ENV=development npx webpack serve",
    "build": "npx webpack",
    "test": "jest --passWithNoTests",
    "provision": "aws cloudformation deploy --region us-east-1 --template-file ./aws.yaml --stack-name super-app-stack --parameter-overrides BucketName=the-super-app AccountId=749936333804",
    "upload": "aws s3 sync dist/ s3://the-super-app --delete",
    "deploy": "npm run build && npm run provision && npm run upload"
  },
  "dependencies": {
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.18",
    "@mui/material": "^5.15.18",
    "@tanstack/react-query": "^5.81.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-refresh": "^0.17.0",
    "react-router-dom": "^6.4.3",
    "react-spinners": "^0.11.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@tanstack/react-query-devtools": "^5.81.2",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.4.3",
    "path": "^0.12.7",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
